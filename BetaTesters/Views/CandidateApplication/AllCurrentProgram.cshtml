@model IEnumerable<CandidateApplicationInspectModel>

@{
    ViewBag.Title = "Applications";
}

<h2 class="text-center">@ViewBag.Title</h2>
<hr />

@if (Model.Any())
{
    <div class="row">
        @foreach (var application in Model)
        {
            <partial name="_CandidateApplicationInspectPartial" model="@application" />
        }
    </div>
}
else
{
    <h3 class="text-center">Your program currently has no applications!</h3>
}